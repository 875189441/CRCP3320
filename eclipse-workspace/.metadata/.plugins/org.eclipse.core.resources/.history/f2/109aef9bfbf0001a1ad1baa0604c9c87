	}
	
	void train(ArrayList<T>  newTokens) {
		

} 
T generate() {
	T newToken=null;
	
	return newToken; 
	
	
}
	ArrayList<T> generate (int length){
		ArrayList<T> newSequence = new ArrayList<T>();
		for(int i=0;i<length;i++) {
			newSequence.add(generate());
		}
		return newSequence;
	}
	
	
}