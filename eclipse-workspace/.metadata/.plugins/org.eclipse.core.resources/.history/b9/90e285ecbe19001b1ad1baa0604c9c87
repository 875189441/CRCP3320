import java.util.ArrayList;
public class Tree<T> extends ProbabilityGenerator<T> {
	
	Node<T> root = new Node<T>();
	int L = 3;
	double pMin = 0.1;
	int sampleSet = 0;
	
public void train() {
	for(int i = 1; i <= L; i++){
		for(int k = 0; k <= newTokens.size() - i; k++) {
			ArrayList<T> curSequence = new ArrayList<T>();
			for(int j = 0; j < i; j++) {
				curSequence.add(newTokens.get(k+j));
				}
				Node<T> nood = new Node<T>(curSequence);
				root.addNode(nood);	
			}
		}
	sampleSet = newTokens.size();
	root.pMinElim(sampleSet, pMin);
}

public void print() {
		root.print();
}
public void clearTree() {
	root.clear();
	root = new Node<T>();
}
}