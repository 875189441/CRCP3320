import java.util.ArrayList;
public class Tree<T> extends ProbabilityGenerator<T> {
	
	Node<T> root = new Node<T>();
	int L = 3;
	
public void train() {
	for(int i = 1; i <= L; i++){
		for(int k = 0; k <= newTokens.size() - i; k++) {
			ArrayList<T> curSequence = new ArrayList<T>();//cursequence creat for each times
			for(int j = 0; j < i; j++) {
				curSequence.add(newTokens.get(k+j));
				}
			//create new node with cursequence
				Node<T> nood = new Node<T>(curSequence);
				root.addNode(nood);	
			}
		//pm:
		}
}

public void print() {
		root.print();
}
public void clearT() {
	root.clear();
	root=new Node<T>();
	
}
}